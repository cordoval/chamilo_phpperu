<?xml version="1.0" encoding="UTF-8"?>
<object name="message_template">
    <properties>
        <property name="id" type="integer" unsigned="1" notnull="1" autoincrement="true" />
        <property name="use_chamilo_headers" type="boolean" default="false" />
        <property name="name" type="text" length="100" fixed="true" notnull="1" default="" />
        <property name="application" type="text" length="50" fixed="true" notnull="1" default="" />
        <property name="message" type="text" />
    </properties>
    <index name="name">
        <indexproperty name="name" />
    </index>
    <index name="application">
        <indexproperty name="application" />
    </index>
</object>