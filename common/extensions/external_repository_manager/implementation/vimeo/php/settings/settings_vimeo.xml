<?xml version="1.0" encoding="UTF-8"?>
<application name="vimeo">
	<settings>
		<category name="credentials">
			<setting name="consumer_key" field="text" default="">
				<validations>
					<validation rule="required" message="ThisFieldIsRequired" />
				</validations>
			</setting>
			<setting name="consumer_secret" field="text" default="">
				<validations>
					<validation rule="required" message="ThisFieldIsRequired" />
				</validations>
			</setting>
			<setting name="oauth_token" field="text" default="" user_setting="1"/>
			<setting name="oauth_token_secret" field="text" default="" user_setting="1"/>
			
		</category>
	</settings>
</application>