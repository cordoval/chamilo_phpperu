<?xml version="1.0" encoding="UTF-8"?>
<application name="survey">
	<reporting_block name="SurveyQuestionAnswers"
		application="survey" function="getSurveyQuestionAnswers" displaymode="Table"
		exclude_displaymodes="Charts, Text" width="500px" height="500px" />
