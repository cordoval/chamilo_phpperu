<?xml version="1.0" encoding="UTF-8"?>
<application name="weblcms">
	<settings>
		<category name="general">
			<setting name="create_example_material" field="radio"
				default="0">
				<options type="static">
					<option name="true" value="1" />
					<option name="false" value="0" />
				</options>
			</setting>
			<setting name="show_subscribe_button_on_course_home" field="checkbox" default="1">
				<options type="static">
					<option name="show_subscribe_button_on_course_home" value="1" />
				</options>
			</setting>
		</category>
		<category name = "viewer">
			<setting name="view_state" field="radio" default="New_View" user_setting="1">
				<options type="static">
					<option name="New" value="New_View" />
					<option name="Old" value="Old_View" />
				</options>
			</setting>
			<setting name="new_view_state_sub_division" field="radio" default="Mixed" user_setting="1">
				<options type="static">
					<option name="Open and closed mixed" value="Mixed" />
					<option name="Open and closed seperated" value="Seperated" />
					<option name="Open only" value="OpenOnly" />
				</options>
			</setting>
		</category>
	</settings>
</application>