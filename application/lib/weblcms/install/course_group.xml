			unsigned="1" notnull="1" default="0" />
		<property name="self_unreg_allowed" type="integer" length="1"
			unsigned="1" notnull="1" default="0" />
	</properties>
	<index name="id" type="primary">
		<indexproperty name="id" />
	</index>
	<index name="course_id">
		<indexproperty name="course_id" />
	</index>
</object>