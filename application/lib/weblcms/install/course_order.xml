<?xml version="1.0" encoding="UTF-8"?>
<object name="course_order">
	<properties>
		<property name="course_id" type="integer" notnull="1" />
		<property name="title" type="text" length="50"  fixed="true" />
		<property name="name_user" type="text" length="50"  fixed="true" />
