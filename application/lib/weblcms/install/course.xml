<?xml version="1.0" encoding="UTF-8"?>
<object name="course">
	<properties>
		<property name="id" type="integer" notnull="1" autoincrement="true" />
		<property name="course_type_id" type="integer" default="0"/>
		<property name="title" type="text" length="250" fixed="true" />
		<property name="category_id" type="integer" notnull="1" default="0"/>
		<property name="titular_id" type="integer" />
		<property name="visual_code" type="text" length="40" fixed="true"
			notnull="1" />
		<property name="department_name" type="text" length="30"
			fixed="true" />
		<property name="department_url" type="text" length="180"
			fixed="true" />
		<property name="last_visit" type="timestamp" />
		<property name="last_edit" type="timestamp" />
		<property name="creation_date" type="timestamp" />
		<property name="expiration_date" type="timestamp" />
	</properties>
	<index name="course_type_id">
		<indexproperty name="category_id" />
	</index>
	<index name="visual_code" type="unique">
		<indexproperty name="visual_code" />
	</index>
	<index name="category_id">
		<indexproperty name="category_id" />
	</index>
	<index name="titular_id">
		<indexproperty name="titular_id" />
	</index>
</object>