<?xml version="1.0" encoding="UTF-8"?>
<object name="course_request">
	<properties>
		<property name="course_id" type="integer" notnull="1" />
		<property name="title" type="text" length="50"  fixed="true" />
		<property name="course_name" type="text" length="50" fixed="true" />
		<property name="name_user" type="text" length="50"  fixed="true" />
		<property name="motivation" type="text" />
		<property name="creation_date" type="timestamp" />
		<property name="allowed_date" type="integer" unsigned="1" />
	</properties>
	<index name="course_id" type="primary">
		<indexproperty name="course_id" />
	</index>
</object>
