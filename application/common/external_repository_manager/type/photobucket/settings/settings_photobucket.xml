<?xml version="1.0" encoding="UTF-8"?>
<application name="photobucket">
	<settings>
		<category name="credentials">
			<setting name="consumer_key" field="text">
				<validations>
					<validation rule="required" message="ThisFieldIsRequired" />
				</validations>
			</setting>
			<setting type="password" name="consumer_secret" field="text">
				<validations>
					<validation rule="required" message="ThisFieldIsRequired" />
				</validations>
			</setting>
			<setting name="session_token" field="text" user_setting="1" />
		</category>
	</settings>
</application>