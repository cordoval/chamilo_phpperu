<?xml version="1.0" encoding="UTF-8"?>
<object name="calendar_event">
	<properties>
		<property name="id" type="integer" unsigned="1" notnull="1" />
		<!--
			start_date & end_date are defined as integers here. Shouldn't they be
			defined as timestamp like in the content_object.xml file?
		-->
		<property name="start_date" type="integer" unsigned="1"
			notnull="1" />
		<property name="end_date" type="integer" unsigned="1"
			notnull="1" />
		<property name="repeat_type" type="integer" length="1"
			unsigned="1" notnull="1" />
		<property name="repeat_to" type="integer" unsigned="1"
			notnull="1" />
	</properties>
	<index name="id" type="primary">
		<indexproperty name="id" />
	</index>
</object>