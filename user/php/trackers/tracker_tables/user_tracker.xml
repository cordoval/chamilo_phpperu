<?xml version="1.0" encoding="UTF-8"?>
<object name="user_tracker">
	<properties>
		<property name="id" type="integer" unsigned="1" notnull="1"
			autoincrement="true" />
		<property name="type" type="text" length="50" fixed="true"
			notnull="1" />
		<property name="name" type="text" length="150" fixed="true"
			notnull="1" />
		<property name="value" type="text" length="255" fixed="true"
			notnull="1" />
	</properties>
	<index name="typename">
		<indexproperty name="name" />
		<indexproperty name="type" />
	</index>
</object>