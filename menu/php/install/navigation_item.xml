		<property name="category_id" type="integer" unsigned="1"
			notnull="1" default="0" />
		<property name="title" type="text" length="255" unsigned="1"
			notnull="1" fixed="true" />
		<property name="sort" type="integer" unsigned="1" notnull="1" />
		<property name="application" type="text" length="255"
			unsigned="1" notnull="0" fixed="true" />
		<property name="section" type="text" length="255" unsigned="1"
			notnull="0" fixed="true" />
		<property name="extra" type="text" length="255" unsigned="1"
			notnull="0" fixed="true" />
		<property name="url" type="text" length="255" unsigned="1"
			default="" fixed="true" />
		<property name="is_category" type="integer" unsigned="1"
			notnull="1" default="0" />
	</properties>
	<index name="category_id">
		<indexproperty name="category_id" />
	</index>
	<index name="application">
		<indexproperty name="application" />
	</index>
	<index name="sort">
		<indexproperty name="sort" />
	</index>
</object>